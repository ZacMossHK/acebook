<h1 id="profile-header">{{profileUsername}}'s profile</h1>
<form
  id="friend-request"
  action="/friends/requests/new/{{profileUsername}}"
  method="PUT"
>
  <input id="add-friend-button" type="submit" value="Add Friend" />

</form>
<ul class="friends">
  Friends:
  {{#each friends}}
    <li>{{this}}</li>
  {{/each}}
</ul>